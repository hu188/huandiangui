
.map {
  width: 100vw;
  /* height: calc(100vh - 55px ) */
  height: 100vh;
}

.map-view {
  width: 100%;
  height: 550rpx;
  position:fixed;
  top:0;
}
.dialog {
  position: fixed;
  height: 200rpx;
  width: 100%;
  display: flex;
  flex-direction: row; 
  background: #fff;
  bottom:0px;
  border-top: 1rpx solid #eee;
  z-index: 112
}

.dialog-un {
  height: 0;
  width: 0;
}

.dialog-text{
  width: 75%;
  padding-left: 20rpx;
  padding-top: 30rpx;

}
.dialog-but {
  width: 25%;
}
.device-name {
  font-size: 22px;
  color: #333;
}
.device-address {
  font-size: 14px;
  color: #999;
  padding-top: 15rpx;
}

.but{
  border: 1rpx solid #78B9AC;
  border-radius: 230rpx;
  width: 150rpx;
  height: 60rpx;
  text-align: center;
  color: #78B9AC;
  font-size: 24rpx;
  font-weight: bold;
  line-height: 60rpx;
  margin-top: 80rpx;
}

.initLoad {
  z-index: 9999;
  background: #fff;
  position: fixed;
  width: 100vw;
  height: 100vh;
  top: 0;
}


.initLoad-mark {
  display: flex;
  margin-top: 30px;
  justify-content: center;
}

.initLoad-mark image {
  width: calc(342rpx * 0.6);
  height: calc(292rpx * 0.6);
}

.initLoad-load {
  display: flex;
  margin-top: 18px;
  justify-content: center;
}

.auth-opt-out{
  background: rgb(205, 228, 223);
  width: 60px;
  height: 60px;
  position: fixed;
  bottom: 100px;
  left: calc(50vw - 30px)
}

.auth-opt-in{
  background: rgb(32, 34, 34);
  width: 62px;
  height: 62px;
}

.auth-opt {
  background: #78B9AC;
  width: 60px;
  height: 60px;
  border-radius: 50%;
  color: #FFF;
  line-height: 60px;
  font-size: 16px;
  text-align: center;  
}
