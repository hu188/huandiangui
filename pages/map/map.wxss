.modalText {
   font-size: 32rpx;
   color: chocolate;
   margin-bottom: 30rpx;
}

.modalDlg{
  width: 580rpx;
  height: 160rpx;
  position: fixed;
  padding-top: 30rpx;
  top: 40%; 
  left: 10%;
  /* z-index: 9999; */
  /* margin: -370rpx 85rpx; */
  background-color: #fff;
  border-radius: 36rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  overflow: hidden; 
}

/** 修改button默认的点击态样式类**/
.modalButton {
   width:100%;
   font-family: fantasy;
   border-radius: 0;
}

.modalButton::after {
   border:none;
   border-radius: 0;
}
